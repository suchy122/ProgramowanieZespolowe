<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="600.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="Controller.Employee.EmployeeMainController">
    <children>
        <VBox prefHeight="100.0" prefWidth="1200.0">
            <children>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <ImageView fitHeight="80.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </HBox.margin>
                            <image>
                                <Image url="/img/icon/logo.png"/>
                            </image>
                        </ImageView>
                        <Label fx:id="Title" prefHeight="40.0" prefWidth="194.0" text="Sklep ">
                            <font>
                                <Font size="18.0"/>
                            </font>
                            <HBox.margin>
                                <Insets top="30.0"/>
                            </HBox.margin>
                        </Label>
                        <Label fx:id="userNameText" prefHeight="40.0" prefWidth="150.0" text="Label">
                            <HBox.margin>
                                <Insets left="350.0" top="30.0"/>
                            </HBox.margin>
                            <font>
                                <Font size="18.0"/>
                            </font>
                        </Label>
                        <Button fx:id="CartButton" mnemonicParsing="false" onAction="#cart" prefHeight="40.0"
                                prefWidth="100.0" text="Koszyk">
                            <HBox.margin>
                                <Insets left="25.0" top="30.0"/>
                            </HBox.margin>
                        </Button>
                        <Button fx:id="setingButton" mnemonicParsing="false" onAction="#seting" prefHeight="40.0"
                                prefWidth="100.0" text="Ustawienia">
                            <HBox.margin>
                                <Insets left="25.0" top="30.0"/>
                            </HBox.margin>
                        </Button>
                        <Button fx:id="logoutButton" mnemonicParsing="false" onAction="#logout" prefHeight="40.0"
                                prefWidth="100.0" text="Wyloguj">
                            <HBox.margin>
                                <Insets left="25.0" top="30.0"/>
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
        <VBox layoutY="100.0" prefHeight="50.0" prefWidth="1200.0">
            <children>
                <HBox fx:id="menuButton" prefHeight="50.0" prefWidth="970.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#assortment" prefHeight="50.0" prefWidth="400.0"
                                text="Asortyment"/>
                        <Button mnemonicParsing="false" onAction="#orders" prefHeight="50.0" prefWidth="400.0"
                                text="ZamÃ³wienia"/>
                        <Button mnemonicParsing="false" onAction="#shop" prefHeight="50.0" prefWidth="400.0"
                                text="Sklep"/>
                    </children>
                </HBox>
            </children>
        </VBox>
        <Pane fx:id="userView" layoutY="150.0" prefHeight="450.0" prefWidth="1200.0"/>
    </children>
</AnchorPane>

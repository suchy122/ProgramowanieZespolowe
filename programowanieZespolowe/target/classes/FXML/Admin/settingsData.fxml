<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="UserData" prefHeight="400.0" prefWidth="850.0" stylesheets="/CSS/css.css"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Controller.Admin.SettingsData">
    <children>
        <VBox layoutX="78.0" layoutY="14.0" prefHeight="244.0" prefWidth="263.0">
            <children>
                <Label text="Imie:"/>
                <TextField fx:id="SettingsName"/>
                <Label text="Nazwisko:"/>
                <TextField fx:id="SettingsSureName"/>
                <Label text="Adres:"/>
                <TextField fx:id="SettingsAddress"/>
                <Label text="Numer tel:"/>
                <TextField fx:id="SettingsPhoneNumber"/>
                <Label text="Email:"/>
                <TextField fx:id="SettingsEmail"/>
            </children>
        </VBox>
        <VBox layoutX="480.0" layoutY="14.0" prefHeight="174.0" prefWidth="282.0">
            <children>
                <Label text="Login:"/>
                <TextField fx:id="SettingsLogin"/>
                <Label text="Hasło:"/>
                <PasswordField fx:id="SettingsPassword"/>
                <Label text="Nowe hasło:"/>
                <PasswordField fx:id="SettingsNewPassword"/>
            </children>
        </VBox>
        <HBox layoutX="143.0" layoutY="318.0" prefHeight="34.0" prefWidth="133.0">
            <children>
                <Button mnemonicParsing="false" onAction="#SettingsEdit" text="Zmień dane"/>
            </children>
        </HBox>
        <HBox layoutX="510.0" layoutY="209.0" prefHeight="34.0" prefWidth="245.0">
            <children>
                <Button mnemonicParsing="false" onAction="#PasswordChange" prefHeight="26.0" prefWidth="202.0"
                        text="Zmień hasło"/>
            </children>
        </HBox>
    </children>
</AnchorPane>

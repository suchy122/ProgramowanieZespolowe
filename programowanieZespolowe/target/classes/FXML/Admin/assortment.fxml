<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Controller.Admin.Assortment">
    <children>
        <TabPane prefHeight="450.0" prefWidth="1200.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab styleClass="menuButton" text="Procesor">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefWidth="200.0">
                            <children>
                                <TableView fx:id="processorTable" prefHeight="422.0" prefWidth="1200.0">
                                    <columns>
                                        <TableColumn fx:id="processorImg" maxWidth="100.0" minWidth="100.0"
                                                     prefWidth="100.0" text="Obrazek"/>
                                        <TableColumn fx:id="processorName" maxWidth="150.0" minWidth="150.0"
                                                     prefWidth="150.0" styleClass="info" text="Nazwa"/>
